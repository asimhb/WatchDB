<Window x:Class="WatchCollection.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WatchCollection"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Button x:Name="btnView" Width="178" Height="62" VerticalAlignment="Bottom" HorizontalAlignment="Left" Margin="48,0,0,50" RenderTransformOrigin="-0.588,0.566" Click="btnView_Click" Content="Watch Info"></Button>
        <Button x:Name="btnInsert" Width="178" Height="62" VerticalAlignment="Bottom" HorizontalAlignment="Left" Margin="321,0,0,50" RenderTransformOrigin="-0.588,0.566" Click="btnInsert_Click" Content=" Insert Watch"></Button>
        <Button x:Name="btnAdmin" Width="178" Height="62" VerticalAlignment="Bottom" HorizontalAlignment="Left" Margin="547,0,0,50" RenderTransformOrigin="-0.588,0.566" Click="btnAdmin_Click" Content="Admin"></Button>


    </Grid>
</Window>
